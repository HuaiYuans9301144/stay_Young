<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".WelcomeMainActivity">

    <ImageView
        android:id="@+id/welcomeImage1"
        android:layout_width="163dp"
        android:layout_height="101dp"
        android:layout_marginTop="250dp"
        android:layout_marginBottom="9dp"
        android:contentDescription="@string/welcomeImage1"
        app:layout_constraintBottom_toTopOf="@+id/welcomeImage2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/sun" />

    <ImageView
        android:id="@+id/welcomeImage2"
        android:layout_width="337dp"
        android:layout_height="105dp"
        android:layout_marginTop="10dp"
        android:contentDescription="@string/welcomeImage2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/welcomeImage1"
        app:srcCompat="@drawable/stayyoung" />

</androidx.constraintlayout.widget.ConstraintLayout>